pipeline {
    agent any

    stages {

        stage('clone') {
            steps {
                echo "cloning the repo"
            }
        }

        stage('build') {
            steps {
                echo "this is build stage"
            }
        }

        stage('test') {
            steps {
                echo "this is test stage"
            }
        }

        stage('deploy') {
            steps {
                echo "this is deploy stage"
            }
        }

    }
}
